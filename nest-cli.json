{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/kolaybul-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/kolaybul-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/kolaybul-api-gateway",
  "projects": {
    "kolaybul-api-gateway": {
      "type": "application",
      "root": "apps/kolaybul-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/kolaybul-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kolaybul-api-gateway/tsconfig.app.json"
      }
    },
    "listing": {
      "type": "application",
      "root": "apps/listing",
      "entryFile": "main",
      "sourceRoot": "apps/listing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/listing/tsconfig.app.json"
      }
    },
    "booking": {
      "type": "application",
      "root": "apps/booking",
      "entryFile": "main",
      "sourceRoot": "apps/booking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/booking/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "review": {
      "type": "application",
      "root": "apps/review",
      "entryFile": "main",
      "sourceRoot": "apps/review/src",
      "compilerOptions": {
        "tsConfigPath": "apps/review/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    }
  }
}